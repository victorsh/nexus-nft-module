{
  "full": true,
  "plugins": [
    { "cleanupAttrs": true },
    { "inlineStyles": true },
    { "removeDoctype": true },
    { "removeXMLProcInst": true },
    { "removeComments": true },
    { "removeMetadata": true },
    { "removeTitle": true },
    { "removeDesc": true },
    { "removeUselessDefs": true },
    { "removeXMLNS": true },
    { "removeEditorsNSData": true },
    { "removeEmptyAttrs": true },
    { "removeHiddenElems": true },
    { "removeEmptyText": true },
